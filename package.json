    "@vitest/coverage-v8": "^4.0.8",
    "babel-plugin-styled-components": "^2.1.4",
    "eslint": "^9.39.1",
    "eslint-config-next": "16.0.1",
    "eslint-plugin-storybook": "^10.0.7",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "playwright": "^1.56.1",
    "prettier": "3.6.2",
    "storybook": "^10.0.7",
    "typescript": "^5",
    "vitest": "^4.0.8"
